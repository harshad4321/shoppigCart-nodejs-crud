<section>
    <div class="container">
        <div class="row mt-4 ">
            <a href="/admin/add-product" class="btn btn-success ml-auto">Add product</a>
        </div>
        <table class="table mt-5">
  <thead>
    <tr>
      <th scope="col">NO</th>
      <th scope="col">Title</th>
      <th scope="col">Brand</th>
      <th scope="col">category</th>
      <th scope="col">Description:</th>
      <th>images</th>
    </tr>
  </thead>
  <tbody>
      {{#each products}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.Name}}</td>
      <td>{{this.Brand}}</td>
      <td>{{this.Category}}</td>
      <td>{{this.Description}}</td>
      <td><img  style="width:60px; height:60px", src="//{{this.insertedId}}.jpg", alt=""></td> 
    </tr>
    {{/each}}
  </tbody>
</table> 
    </div>
</section>
